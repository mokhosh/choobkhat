{
    "name": "mokhosh/choobkhat",
    "type": "project",
    "description": "Choobkhat for your time tracking needs",
    "keywords": [
        "laravel",
        "filament",
        "time tracking"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.4",
        "filament/filament": "^3.2",
        "laravel/framework": "^11.9",
        "laravel/tinker": "^2.9",
        "mokhosh/filament-jalali": "^3.3",
        "anodyne/blade-tabler-icons": "^3.11",
        "spatie/laravel-model-states": "^2.7",
        "spatie/laravel-tags": "^4.6"
    },
    "require-dev": {
        "driftingly/rector-laravel": "^1.2",
        "fakerphp/faker": "^1.23",
        "laravel-lang/common": "^6.4",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.26",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.0",
        "pestphp/pest": "^2.34",
        "pestphp/pest-plugin-laravel": "^2.4",
        "rector/rector": "^1.2",
        "spatie/laravel-stubs": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
